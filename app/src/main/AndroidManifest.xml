<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
  package="edu.cnm.deepdive.teacherparentnotficationapp">
<uses-permission android:name="android.permission.SEND_SMS"/>
  <application
    android:allowBackup="true"
    android:icon="@mipmap/ic_launcher"
    android:label="@string/app_name"
    android:roundIcon="@mipmap/ic_launcher_round"
    android:supportsRtl="true"
    android:theme="@style/AppTheme">
    <activity
      android:label="@string/app_name"
      android:name="edu.cnm.deepdive.teacherparentnotficationapp.edu.cnm.deepdive.parentnotficationapp.StudentListActivity"
      android:theme="@style/AppTheme.NoActionBar">
      <intent-filter>
        <action android:name="android.intent.action.MAIN"/>

        <category android:name="android.intent.category.LAUNCHER"/>
      </intent-filter>
    </activity>
    <activity
      android:label="@string/title_student_detail"
      android:name="edu.cnm.deepdive.teacherparentnotficationapp.edu.cnm.deepdive.parentnotficationapp.StudentDetailActivity"
      android:parentActivityName="edu.cnm.deepdive.teacherparentnotficationapp.edu.cnm.deepdive.parentnotficationapp.StudentListActivity"
      android:theme="@style/AppTheme.NoActionBar">
      <meta-data
        android:name="android.support.PARENT_ACTIVITY"
        android:value="edu.cnm.deepdive.teacherparentnotficationapp.edu.cnm.deepdive.parentnotficationapp.StudentListActivity"/>
    </activity>
  </application>

</manifest>